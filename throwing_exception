#include<iostream>
using namespace std;
int main()
{
    cout<<"swati"<<endl;
    int a,b;
    cout<<"enter values of a and b:";
    cin>>a;
    cin>>b;
    int x=a-b;
    try
    {
        if(x!=0)
        {
            cout<<"result(a/x)="<<a/x<<endl;
        }
        else
        {
            throw(x);
        }
    }
    catch(int i)
    {
        cout<<"exception caught:x="<<x<<endl;
    }
    cout<<"END";
    return 0;
}
